<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.housekeeping.dao.UserMapper">
    <select id="SelectLoginByName" resultType="com.project.housekeeping.pojo.Login">
        SELECT * FROM `login` WHERE `login_name` = #{username} AND `password` = #{password}
    </select>

</mapper>